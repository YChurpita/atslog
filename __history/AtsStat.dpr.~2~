program AtsStat;

uses
  Vcl.Forms,
  Main in 'Main.pas' {MainForm},
  MainDm in 'MainDm.pas' {DMain: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TMainForm, MainForm);
  Application.CreateForm(TDMain, DMain);
  Application.Run;
end.
